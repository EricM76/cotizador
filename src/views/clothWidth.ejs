                                                    <th scope="row" class="text-secondary">
                                                        âˆ˜  <%= item.name %>
                                                    </th>
                                                    <td>
                                                         <%= item.width %>
                                                    </td>
                                                </tr>
                                                <% }) %>
                                        </tbody>
                                    </table>
                               

                            </div>
                        </div>


                        </div>
                    </article>
                </section>
            </main>
            <%- include('partials/footer') %>
